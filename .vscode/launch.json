{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Google App Engine (py2.7)",
            "request": "launch",
            "type": "python",
            "host": "localhost",
            "cwd": "${workspaceFolder}",
            "python": "${env:ProgramFiles(x86)}/Google/Cloud SDK/google-cloud-sdk/platform/bundledpython2/python.exe",
            "program": "${env:ProgramFiles(x86)}/Google/Cloud SDK/google-cloud-sdk/bin/dev_appserver.py",
            "args": [
                "app.yaml",
                "-A", "skojjt",
                "--port", "8080",
                "--admin_port", "8000",
                "--api_port", "56035",
                "--max_module_instances", "1",
                "--enable_console",
                "--skip_sdk_update_check"
            ]
        }
    ]
}