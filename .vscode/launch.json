{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Google App Engine (py2.7)",
            "request": "launch",
            "type": "python",
            "host": "localhost",
            "cwd": "${workspaceFolder}",
            "python": "${command:python.interpreterPath}",
            "program": "${env:ProgramFiles(x86)}/google/google_appengine/dev_appserver.py",
            "args": [
                "app.yaml",
                "-A", "skojjt",
                "--port", "8080",
                "--admin_port", "8000",
                "--api_port", "56035",
                "--enable_console"
            ]
        }
    ]
}